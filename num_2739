import java.util.Scanner;

public class num_2739 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		if (a >= 1 && a <= 9) {
			for (int i = 1; i <= 9; i++) {
				System.out.println(a + " * " + i + " = " + (a * i));
			}
		}
	}

}
